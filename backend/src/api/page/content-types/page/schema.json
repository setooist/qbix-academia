    "section": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "page.hero",
        "page.trust-indicator",
        "page.about-hero",
        "page.who-we-are",
        "page.timeline-item",
        "page.vision",
